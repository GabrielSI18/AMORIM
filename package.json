    "@clerk/nextjs": "^6.35.2",
    "@prisma/client": "^6.19.0",
    "@react-email/components": "^0.5.7",
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-devtools": "^5.90.2",
    "ai": "^5.0.94",
